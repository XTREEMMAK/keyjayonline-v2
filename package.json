{
	"name": "keyjayonline.com-v2",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"cdn:sync": "node scripts/sync-cdn-assets.js",
		"migrate:productions": "node scripts/migrate-productions-drift.js",
		"migrate:category-config": "node scripts/migrate-category-config.js",
		"migrate:galleries": "node scripts/migrate-galleries.js",
		"migrate:audio-playlists": "node scripts/migrate-audio-playlists.js",
		"docker:init": "set -a && . ./.env.docker.local && set +a && docker compose --profile local-db up kjo2_directus kjo2_postgres -d",
		"docker:up": "set -a && . ./.env.docker.local && set +a && docker compose --profile local-db up -d",
		"docker:down": "docker compose --profile local-db down",
		"docker:logs": "docker compose --profile local-db logs -f",
		"docker:build": "set -a && . ./.env.docker.local && set +a && docker compose --profile local-db build --no-cache"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.983.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"dotenv": "^17.2.1",
		"svelte": "^5.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^4.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@directus/sdk": "^20.0.2",
		"@iconify/svelte": "^5.0.1",
		"@sweetalert2/theme-dark": "^5.0.27",
		"canvas-confetti": "^1.9.3",
		"dompurify": "^3.2.6",
		"form-data": "^4.0.5",
		"isomorphic-dompurify": "^2.26.0",
		"mixitup": "^3.3.2",
		"music-metadata": "^11.8.3",
		"node-fetch": "^3.3.2",
		"pg": "^8.16.3",
		"sweetalert2": "^11.22.4",
		"three": "^0.182.0",
		"valibot": "^1.1.0",
		"wavesurfer.js": "^7.10.1"
	}
}
